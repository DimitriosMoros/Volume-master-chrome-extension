{
  "manifest_version": 3,
  "name": "Volume Master",
  "version": "1.0",
  "description": "Control video volume from 0% to 600%",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
      }
    }
  }